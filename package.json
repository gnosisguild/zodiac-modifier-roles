{
  "name": "zodiac-roles-monorepo",
  "private": true,
  "description": "Zodiac Roles monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install && yarn build:evm && yarn workspaces foreach run prepare",
    "pre-commit": "yarn workspaces foreach run pre-commit",
    "test": "yarn workspaces foreach run test",
    "build": "yarn workspaces foreach run build",
    "build:evm": "yarn workspace @gnosis.pm/zodiac-modifier-roles run build",
    "coverage": "yarn workspace @gnosis.pm/zodiac-modifier-roles run coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gnosis/zodiac-modifier-roles.git"
  },
  "author": "",
  "license": "LGPL-3.0+",
  "devDependencies": {
    "husky": "5.1.3"
  },
  "packageManager": "yarn@3.5.1",
  "dependencies": {
    "clsx": "^1.2.1",
    "nextra": "^2.6.2",
    "nextra-theme-docs": "^2.6.2",
    "react-icons": "^4.9.0"
  }
}
