{
  "name": "zodiac-roles-monorepo",
  "private": true,
  "description": "Zodiac Roles monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "pre-commit": "yarn workspaces foreach run pre-commit",
    "test": "turbo test",
    "build": "turbo build",
    "build:evm": "turbo build --filter=@gnosis-guild/zodiac-core-modifier-roles",
    "build:deployments": "turbo build --filter=zodiac-roles-deployments",
    "build:sdk": "turbo build --filter=zodiac-roles-sdk",
    "build:app": "turbo build --filter=zodiac-roles-app",
    "build:docs": "turbo build --filter=zodiac-roles-docs",
    "coverage": "yarn workspace @gnosis-guild/zodiac-core-modifier-roles run coverage",
    "dev": "turbo run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gnosisguild/zodiac-modifier-roles.git"
  },
  "author": "",
  "license": "LGPL-3.0+",
  "packageManager": "yarn@3.7.0",
  "dependencies": {
    "turbo": "^2.5.6"
  },
  "version": "1.0.0"
}
