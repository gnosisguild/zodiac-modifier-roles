{
  "$schema": "https://turbo.build/schema.json",
  "globalPassThroughEnv": [
    "REACT_APP_SUBGRAPH_BSC",
    "REACT_APP_SUBGRAPH_AVALANCHE",
    "REACT_APP_SUBGRAPH_OPTIMISM",
    "REACT_APP_SUBGRAPH_ARBITRUM",
    "REACT_APP_SUBGRAPH_POLYGON",
    "REACT_APP_SUBGRAPH_MAINNET",
    "REACT_APP_SUBGRAPH_GNOSIS_CHAIN",
    "REACT_APP_SUBGRAPH_GOERLI",
    "REACT_APP_SUBGRAPH_BASE_URL",
    "REACT_APP_ONBOARD_JS_DAPP_ID",
    "REACT_APP_INFURA_KEY",
    "REACT_APP_SNOWTRACE_KEY",
    "REACT_APP_OPTIMISTICSCAN_KEY",
    "REACT_APP_BSCSCAN_KEY",
    "REACT_APP_ARBISCAN_KEY",
    "REACT_APP_POLYGONSCAN_KEY",
    "REACT_APP_ETHERSCAN_KEY",
    "KV_URL",
    "KV_REST_API_URL",
    "KV_REST_API_TOKEN",
    "KV_REST_API_READ_ONLY_TOKEN"
  ],
  "tasks": {
    "generate": { "cache": false },
    "test": { "cache": false, "dependsOn": ["build"] },
    "build": {
      "dependsOn": ["clean", "^generate", "^build"],
      "outputs": ["build", "typechain-types"]
    },
    "clean": { "cache": false },
    "dev": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    },
    "dist": { "dependsOn": ["^build"] }
  }
}
