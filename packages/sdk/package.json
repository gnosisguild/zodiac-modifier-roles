{
  "name": "zodiac-roles-sdk",
  "version": "1.0.0",
  "main": "build/cjs/sdk/index.js",
  "typings": "build/cjs/sdk/index.d.ts",
  "module": "build/esm/sdk/index.js",
  "license": "LGPL-3.0+",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "check": "yarn check:prettier && yarn check:lint",
    "check:lint": "eslint src test --ext .ts",
    "check:prettier": "prettier \"src/**/*.ts\" --list-different",
    "fix": "yarn fix:prettier && yarn fix:lint",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "hardhat test",
    "test:simulate": "hardhat test ./test/karpatkey/simulateTransactions.test.ts --grep",
    "apply:dao:gno:manage": "hardhat encodeApplyPresetManage --network xdai --safe DAO_GNO",
    "apply:dao:gno:harvest": "hardhat encodeApplyPresetHarvest --network xdai --safe DAO_GNO",
    "apply:ltd:gno:manage": "hardhat encodeApplyPresetManage --network xdai --safe LTD_GNO",
    "apply:ltd:gno:harvest": "hardhat encodeApplyPresetHarvest --network xdai --safe LTD_GNO",
    "apply:dao:eth:manage": "hardhat encodeApplyPresetManage --safe DAO_ETH",
    "apply:dao:eth:harvest": "hardhat encodeApplyPresetHarvest --safe DAO_ETH",
    "apply:ltd:eth:manage": "hardhat encodeApplyPresetManage --safe LTD_ETH",
    "apply:ltd:eth:harvest": "hardhat encodeApplyPresetHarvest --safe LTD_ETH",
    "pre-commit": "yarn fix"
  },
  "dependencies": {
    "ethers": "5.4.6",
    "ethers-multisend": "^2.1.1"
  },
  "devDependencies": {
    "@gnosis.pm/safe-core-sdk": "^2.1.0",
    "@gnosis.pm/safe-core-sdk-types": "^1.1.0",
    "@gnosis.pm/safe-ethers-lib": "^1.1.0",
    "@gnosis.pm/safe-service-client": "^1.1.2",
    "@nomiclabs/hardhat-ethers": "2.0.0",
    "@nomiclabs/hardhat-etherscan": "2.1.0",
    "@nomiclabs/hardhat-waffle": "2.0.0",
    "@typechain/ethers-v5": "10.0.0",
    "@typechain/hardhat": "6.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.36",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "3.1.4",
    "ethereum-waffle": "3.2.0",
    "hardhat": "^2.9.7",
    "hardhat-deploy": "0.7.0-beta.38",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.0",
    "typechain": "8.0.0",
    "typescript": "^4.7.2",
    "yargs": "^16.1.1"
  }
}
