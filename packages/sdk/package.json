{
  "name": "zodiac-roles-sdk",
  "version": "2.21.1",
  "license": "LGPL-3.0+",
  "main": "build/cjs/sdk/src/index.js",
  "module": "build/esm/sdk/src/index.js",
  "types": "build/cjs/sdk/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./build/cjs/sdk/src/index.js",
      "import": "./build/esm/sdk/src/index.js"
    },
    "./kit": {
      "require": "./build/cjs/sdk/src/entrypoints/kit.js",
      "import": "./build/esm/sdk/src/entrypoints/kit.js"
    },
    "./annotations": {
      "require": "./build/cjs/sdk/src/entrypoints/annotations.js",
      "import": "./build/esm/sdk/src/entrypoints/annotations.js"
    },
    "./typechain": {
      "require": "./build/cjs/sdk/src/entrypoints/typechain.js",
      "import": "./build/esm/sdk/src/entrypoints/typechain.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./build/cjs/sdk/src/index.d.ts"
      ],
      "kit": [
        "./build/cjs/sdk/src/entrypoints/kit.d.ts"
      ],
      "annotations": [
        "./build/cjs/sdk/src/entrypoints/annotations.d.ts"
      ],
      "typechain": [
        "./build/cjs/sdk/src/entrypoints/typechain.d.ts"
      ]
    }
  },
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf build && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "yarn check:types && yarn check:prettier && yarn check:lint",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint src test --ext .ts",
    "check:prettier": "prettier \"src/**/*.ts\" --list-different",
    "fix": "yarn fix:prettier && yarn fix:lint",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "pre-commit": "yarn fix",
    "prepare": "yarn eth-sdk"
  },
  "dependencies": {
    "ethers": "^6.13.2",
    "zodiac-roles-deployments": "workspace:^"
  },
  "peerDependencies": {
    "@gnosis-guild/eth-sdk-client": "^0.1.6",
    "openapi-backend": "^5.11.0",
    "openapi-types": "^12.1.3"
  },
  "peerDependenciesMeta": {
    "@gnosis-guild/eth-sdk-client": {
      "optional": true
    },
    "openapi-backend": {
      "optional": true
    },
    "openapi-types": {
      "optional": true
    }
  },
  "devDependencies": {
    "@gnosis-guild/eth-sdk": "^0.3.7",
    "@gnosis-guild/eth-sdk-client": "^0.1.6",
    "@gnosis-guild/typechain-ethers-v6": "^0.5.4",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "openapi-backend": "^5.11.0",
    "openapi-types": "^12.1.3",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.0",
    "typechain": "^8.3.2",
    "typescript": "^5.6.2",
    "vitest": "3.0.4"
  },
  "resolutions": {
    "typechain": "^8.3.2"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
