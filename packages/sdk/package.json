{
  "name": "zodiac-roles-sdk",
  "version": "3.3.3",
  "license": "LGPL-3.0+",
  "description": "A library for managing role permissions on Zodiac Roles Modifier",
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    },
    "./annotations": {
      "import": "./build/annotations.mjs",
      "require": "./build/annotations.js"
    },
    "./kit": {
      "import": "./build/kit.mjs",
      "require": "./build/kit.js"
    },
    "./swaps": {
      "import": "./build/swaps.mjs",
      "require": "./build/swaps.js"
    },
    "./typechain": {
      "import": "./build/typechain.mjs",
      "require": "./build/typechain.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./build/index.d.ts"
      ],
      "kit": [
        "./build/kit.d.ts"
      ],
      "annotations": [
        "./build/annotations.d.ts"
      ],
      "swaps": [
        "./build/swaps.d.ts"
      ],
      "typechain": [
        "./build/typechain.d.ts"
      ]
    }
  },
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf build",
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "yarn check:types && yarn check:prettier && yarn check:lint",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint src test --ext .ts",
    "check:prettier": "prettier \"src/**/*.ts\" --list-different",
    "fix": "yarn fix:prettier && yarn fix:lint",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "pre-commit": "yarn fix",
    "generate": "yarn eth-sdk"
  },
  "dependencies": {
    "@cowprotocol/cow-sdk": "^7.2.11",
    "ethers": "^6.13.5",
    "zodiac-roles-deployments": "workspace:*"
  },
  "peerDependencies": {
    "@gnosis-guild/eth-sdk-client": "^0.1.6",
    "openapi-backend": "^5.15.0",
    "openapi-types": "^12.1.3"
  },
  "peerDependenciesMeta": {
    "@gnosis-guild/eth-sdk-client": {
      "optional": true
    },
    "openapi-backend": {
      "optional": true
    },
    "openapi-types": {
      "optional": true
    }
  },
  "devDependencies": {
    "@gnosis-guild/eth-sdk": "^0.3.7",
    "@gnosis-guild/eth-sdk-client": "^0.1.6",
    "@gnosis-guild/typechain-ethers-v6": "^0.5.5",
    "@gnosis-guild/zodiac-core-modifier-roles": "workspace:*",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "openapi-backend": "^5.15.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsdown": "^0.14.1",
    "typechain": "^8.3.2",
    "typescript": "^5.7.3",
    "vitest": "3.0.4"
  },
  "resolutions": {
    "typechain": "^8.3.2"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
